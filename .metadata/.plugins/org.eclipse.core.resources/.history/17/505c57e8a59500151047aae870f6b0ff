package com.walter;

public class Account {
	String name;
	String number;
	double balance;

	// constructor to set up values
	public Account(String name, String number, double balance) {
		super();
		this.name = name;
		this.number = number;
		this.balance = balance;
	}

	// getters
	public String getName() {
		return name;
	}
	public String getNumber() {
		return number;
	}
	public double getBalance() {
		return balance;
	}
    
	
	
	
	
	
	
	
	
}
